  <table class="table" style="background: currentColor">
    <thead>
      <tr>
        <th scope="col" style="color: white">Titulo</th>
        <th scope="col" style="color: white">Imagen</th>
        <th scope="col" style="color: white">Precio</th>
      </tr>
    </thead>
    <tbody>
      <% products.forEach(item => { %>
      <tr>
        <td style="color: white" scope="row"><%= item.title %></td>
        <td><img src="<%= item.img %>" /></td>
        <td style="color: white"><%= item.price %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
